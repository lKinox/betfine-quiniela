generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Selection {
  id               String   @id @default(uuid())
  username         String
  email            String
  phone            String
  picks            Json
  paymentProofUrl  String
  createdAt        DateTime @default(now())
}

model User {
  id               String   @id @default(uuid())
  username         String   @unique
  password         String
}